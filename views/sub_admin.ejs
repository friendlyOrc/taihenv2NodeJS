<div class="row">
    <ol class="breadcrumb">
        <li><a href="#"><svg class="glyph stroked home"><use xlink:href="#stroked-home"></use></svg></a></li>
        <li class="active">Trang chủ quản trị</li>
    </ol>
</div>

<div class="row page-header">
    <div class="col-lg-12">
        <h1 >Trang chủ quản trị</h1>
    </div>
</div><!--/.row-->

<div class="row">
    <div class="col-xs-12 col-md-6 col-lg-3">
        <div class="panel panel-blue panel-widget ">
            <div class="row no-padding">
                <div class="col-sm-3 col-lg-5 widget-left">
                    <svg class="glyph stroked tag"><use xlink:href="#stroked-tag"/></svg>
                </div>
                <div class="col-sm-9 col-lg-7 widget-right">
                    <div class="large"><%= sess.articles.length%></div>
                    <div class="text-muted">Đầu sách</div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-xs-12 col-md-6 col-lg-3">
        <div class="panel panel-orange panel-widget">
            <div class="row no-padding">
                <div class="col-sm-3 col-lg-5 widget-left">
                    <svg class="glyph stroked tag"><use xlink:href="#stroked-tag"/></svg>
                </div>
                <div class="col-sm-9 col-lg-7 widget-right">
                    <div class="large"><%= sess.category.length%></div>
                    <div class="text-muted">category</div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-xs-12 col-md-6 col-lg-3">
        <div class="panel panel-teal panel-widget">
            <div class="row no-padding">
                <div class="col-sm-3 col-lg-5 widget-left">
                    <svg class="glyph stroked male-user"><use xlink:href="#stroked-male-user"></use></svg>
                </div>
                <div class="col-sm-9 col-lg-7 widget-right">
                    <div class="large"><%= sess.account.length%></div>
                    <div class="text-muted">Thành Viên</div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-xs-12 col-md-6 col-lg-3">
        <div class="panel panel-red panel-widget">
            <div class="row no-padding">
                <div class="col-sm-3 col-lg-5 widget-left">
                    <svg class="glyph stroked eye"><use xlink:href="#stroked-eye"/></svg>
                </div>
                <div class="col-sm-9 col-lg-7 widget-right">
                    <div class="large"><%= sess.view.length%></div>
                    <div class="text-muted">View</div>
                </div>
            </div>
        </div>
    </div>
</div><!--/.row-->